    <div class="container ">
        <div class="row">

        {# A_SETTINGS menu #}
                <nav class="container navbar navbar-expand-lg static-top">
                    <a class="navbar-brand" href="{{ home }}">
                        <img class="logo"
                             src="{% if custom_logo_url %}{{ custom_logo_url }}{% else %}{{ stylesheet_directory_uri }}/img/logo-astratto-v2-b.png{% endif %}"
                             height="20">
                    </a>
                    <button class="navbar-toggler d-block d-lg-none ml-auto hidden-sm-up float-xs-right" type="button"
                            data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarNav"
                            aria-expanded="false"
                            aria-label="Toggle navigation">
                        <span style="font-size:30px;cursor:pointer" onclick="openNav()"> <i
                                    class="fas fa-bars"></i></span>
                    </button>
                    <div class="collapse navbar-collapse d-none d-lg-block" id="navbarResponsive">
                        {% include "/views/headers/header-2/header-2-menu.twig" with {'primary_menu': primary_menu.get_items} %}
                    </div>
                </nav>
                <!-- /.menu -->


        </div>
    </div>
</section>

